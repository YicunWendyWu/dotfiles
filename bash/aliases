alias g=git
alias ga='git add'
alias gai='git add -i'
alias gap='git add -p'
alias gb='git branch'
alias gbd='git branch -D'
alias gbl='git blame'
alias gc='git commit'
alias gca='git commit --amend'
alias gcf='git cfg'
alias gcl='git clean -fd'
alias gco='git checkout'
alias gcob='git checkout -b'
alias gd='git diff'
alias gdc='git diff --cached'
alias gf='git fetch'
alias gh='git help'
alias gl='git log'
alias gm='git merge'
alias gmv='git mv'
alias gp='git push'
alias gpl='git pull'
alias gr='git reset'
alias grb='git rebase'
alias grbi='git rebase -i'
alias grm='git rm'
alias grmt='git remote'
alias gs='git status'
alias gst='git stash'
alias gstp='git stash pop'
alias gsts='git stash show'
