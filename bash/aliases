alias gc='git commit'
alias gca='git commit --amend'
alias gcf='git cfg'
alias gco='git checkout'
alias gd='git diff'
alias gdc='git diff --cached'
alias gf='git fetch'
alias gh='git help'
alias gl='git log'
alias gm='git merge'
alias gp='git push'
alias gpl='git pull'
alias gr='git reset'
alias grb='git rebase'
alias grbi='git rebase -i'
alias gs='git status'
